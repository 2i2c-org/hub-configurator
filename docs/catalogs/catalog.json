{
  "name": "2i2c Hub Catalog",
  "version": "2025.08.22",
  "items": [
    {
        "id": "cluster:csp",
        "group": "Cluster",
        "label": "Cloud Service Provider",
        "help": "Which cloud we deploy to and operate? 2i2c-managed means 2i2c has full administrator access to cloud provider account.",
        "perTier": {
            "Essential": {
                "kind": "single",
                "text": "AWS (2i2c-managed)"
            },
            "Advanced": {
                "kind": "select",
                "options": [
                    "AWS (2i2c-managed)",
                    "GCP (2i2c-managed)",
                    "AWS (2i2c-managed via cloudbank)",
                    "GCP (2i2c-managed via cloudbank)",
                    "AWS (community-managed, Unrestricted IAM admin access for 2i2c)",
                    "GCP (community-managed, Unrestricted IAM admin access for 2i2c)"
                ],
                "default": "AWS (2i2c-managed)"
            },
            "Premier": {
                "kind": "select",
                "options": [
                    "AWS (2i2c-managed)",
                    "GCP (2i2c-managed)",
                    "AWS (2i2c-managed via cloudbank)",
                    "GCP (2i2c-managed via cloudbank)",
                    "AWS (community-managed)",
                    "GCP (community-managed)",
                    "Azure (community-managed)",
                    "AWS (NASA SMCE, by agreement)",
                    "JetStream2 (by agreement)"
                ],
                "default": "AWS (2i2c-managed)"
            }
        }
    },
    {
        "id": "cluster:billing",
        "group": "Cluster",
        "label": "Cloud Billing",
        "help": "How is the cloud bill paid?",
        "perTier": {
            "Essential": {
                "kind": "single",
                "text": "2i2c holds account; pass-through costs"
            },
            "Advanced": {
                "kind": "select",
                "options": [
                    "2i2c account; pass-through costs",
                    "Customer-owned account with 2i2c access"
